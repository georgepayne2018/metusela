<script type="text/javascript">
	function updateCardContent(element, text) {
    element.innerHTML = text.replace(/\n/g, '<br>'); // Convert \n to <br>
}
	let currentIndex = 0;
let startX = null;

function moveCarousel(direction) {
    const carousel = document.querySelector('.carousels');
    const items = document.querySelectorAll('.carousel-itemer');
    const totalItems = items.length;

    currentIndex += direction;

    if (currentIndex < 0) {
        currentIndex = totalItems - 1;
    } else if (currentIndex >= totalItems) {
        currentIndex = 0;
    }

    const offset = -currentIndex * 100;
    carousel.style.transform = `translateX(${offset}%)`;
}

function handleTouchStart(event) {
    startX = event.touches[0].clientX;
}

function handleTouchMove(event) {
    if (startX === null) {
        return;
    }

    const currentX = event.touches[0].clientX;
    const diffX = startX - currentX;

    if (Math.abs(diffX) > 50) {
        moveCarousel(diffX > 0 ? 1 : -1);
        startX = null;
    }
}

document.querySelector('.carousels').addEventListener('touchstart', handleTouchStart);
document.querySelector('.carousels').addEventListener('touchmove', handleTouchMove);

</script>
