<script type="text/javascript">
	document.getElementById('notifyBtn').addEventListener('click', function() {
    var notification = document.getElementById('notification');
    notification.classList.add('show');
});

document.getElementById('closeBtn').addEventListener('click', function() {
    var notification = document.getElementById('notification');
    notification.classList.remove('show');
});
	document.addEventListener('DOMContentLoaded', () => {
  document.body.addEventListener('submit', function(event) {
  	var notifElement = document.getElementById('notif');
  	var notifElements = document.getElementById('notification');
    if (event.target.classList.contains('delete-tweet-form')) {
      event.preventDefault(); // Prevent the default form submission
     
      const formData = new FormData(event.target);

      fetch('/delete-tweet', {
        method: 'POST',
        body: formData,
      })
      .then(response => response.json())
      .then(data => {
        const notify = document.getElementById('notifyBtn');
        if (data.success) {
          notifElements.className = 'alert alert-success notification  '; // Add success class
          notifElement.innerHTML = data.message;
        } else {
          notifElements.className = 'alert alert-danger notification  '; // Add danger class
          notifElement.innerHTML = data.message;
        }
        document.getElementById('notifyBtn').click();
      })
      .catch(error => {
        notifElements.className = 'alert alert-danger notification  '; // Add danger class for error
        notifElement.innerHTML = 'An error occurred. Please try again.';
        document.getElementById('notifyBtn').click();
      });
    }
  });
});

</script>




